Name:         vmi-system-datasource
Namespace:    verrazzano-system
Labels:       k8s-app=verrazzano.io
              vmo.v1.verrazzano.io=system
Annotations:  <none>

Data
====
datasource.yaml:
----

apiVersion: 1

datasources:
- name: Prometheus
  type: prometheus
  orgId: 1
  access: proxy
  url: http://vmi-system-prometheus:9090
  isDefault: true
- name: Prometheus AlertManager
  type: camptocamp-prometheus-alertmanager-datasource
  orgId: 1
  access: proxy
  url: http://vmi-system-alertmanager:9093
  isDefault: false
  jsonData:
    severity_critical: "4"
    severity_high: "3"
    severity_warning: "2"
    severity_info: "1"
Events:  <none>
