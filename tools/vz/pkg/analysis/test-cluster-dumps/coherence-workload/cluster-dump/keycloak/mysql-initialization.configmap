Name:         mysql-initialization
Namespace:    keycloak
Labels:       app.kubernetes.io/managed-by=Helm
Annotations:  meta.helm.sh/release-name: mysql
              meta.helm.sh/release-namespace: keycloak

Data
====
create-db.sql:
----
CREATE DATABASE IF NOT EXISTS keycloak DEFAULT CHARACTER SET utf8 DEFAULT COLLATE utf8_general_ci;USE keycloak;GRANT CREATE, ALTER, DROP, INDEX, REFERENCES, SELECT, INSERT, UPDATE, DELETE ON keycloak.* TO 'keycloak'@'%';FLUSH PRIVILEGES;
